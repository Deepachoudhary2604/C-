
#include <iostream>
using namespace std;
int main(){
    int size;
    cout<<"enter size";
    cin>>size;
    int arr[size];
    for (int i=0;i<size;i++){
        cin>>arr[i];
    }
    int key;
    cout<<"enter element you want to count";
    cin>>key;
    int count=0;
    for (int i=0;i<size;i++){
        if (arr[i]==key){
            count++;
        }
    }
    cout<<count;
    
}
