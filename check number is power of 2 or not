
#include <iostream>
using namespace std;
#include <cmath>
int power(int n){
    for (int i=0;i<31;i++){
        int ans=pow(2,i);
        if (n==ans){
            return true;
            break;
        }
    }
    return false;
}
int main() {
    int n;
    cin>>n;
    int poweroftwo=power(n);
    cout<<poweroftwo;
}
